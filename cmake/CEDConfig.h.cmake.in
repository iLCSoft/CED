#ifndef CEDConfig_h
#define CEDConfig_h 1

// version macros
#define CED_MAJOR_VERSION @CED_VERSION_MAJOR@
#define CED_MINOR_VERSION @CED_VERSION_MINOR@
#define CED_PATCH_LEVEL @CED_VERSION_PATCH@

#define CED_VERSION_GE( MAJV , MINV , PLEV )  ( (CED_MAJOR_VERSION > MAJV) || ( (CED_MAJOR_VERSION == MAJV) && (CED_MINOR_VERSION > MINV) ) || ( (CED_MAJOR_VERSION == MAJV) && (CED_MINOR_VERSION == MINV) && (CED_PATCH_LEVEL >= PLEV) ) )

#endif // CEDConfig_h
